{
    "Serilog": {
        "MinimumLevel": {
            "Default": "Information",
            "Override": {
                "System": "Warning",
                "Microsoft": "Warning"
            }
        },
        "Enrich": [ "FromLogContext" ],
        "WriteTo": {
            "Console": {
                "Name": "Console",
                "Args": {
                    "outputTemplate": "[{Level:u3} {Timestamp:HH:mm:ss.fffffffK}] {Scope} {Message:lj}{NewLine}{Exception}"
                }
            }
        }
    },
    "AllowedHosts": "*",
    "CodeChallengeUrl": "https://codechallenge.boohma.com"
}
